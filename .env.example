# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# LangSmith
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=email-assistant

# Google / Gmail
# Path to OAuth token (created after first login). Can use .secrets/ or .secret/ folder.
GOOGLE_TOKEN_PATH=.secrets/token.json
# Path to credentials JSON from Google Cloud Console. Can use .secret/credentials.json if you use a .secret folder.
GOOGLE_CREDENTIALS_PATH=.secrets/credentials.json

# Supabase
SUPABASE_URL=
SUPABASE_KEY=

# Postgres (checkpointer + optional store)
DATABASE_URL=

# Phase 3: user id for persisted chats (optional; default: default-user)
USER_ID=
